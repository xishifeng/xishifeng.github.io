{
	"manifest_version": 2, //固定的
	"name": "xishifeng", //插件名称
	"version": "1.0", //插件使用的版本
	"description": "The first extension that CC made.", //插件的描述
	"icons": {
		"128": "image/autoF5Icon.png"
	},
	"permissions": [
		"http://*.fujuhaofang.com/*",
		"https://*.fujuhaofang.com/*",
		"http://*.tonglianjituan.com/*",
		"https://*.tonglianjituan.com/*"
	],
	"background": {
		"scripts": [
			"js/jquery.min.js",
			"js/background.js"
		]
	},
	"browser_action": {//插件加载后生成图标
		"default_icon": "image/autoF5Icon2.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://dev0.fujuhaofang.com/meeting_pc/meeting_app.html"
			],
			"js": [
				"js/jquery.min.js",
				"js/content.js"
			],
			"run_at": "document_idle"
		}
	]
}